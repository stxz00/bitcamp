<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>day11</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
	<servlet>
		<servlet-name>emplist</servlet-name>
		<servlet-class>com.bit.xml.Emp</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>empAdd</servlet-name>
		<servlet-class>com.bit.xml.EmpAdd</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>empOne</servlet-name>
		<servlet-class>com.bit.xml.EmpOne</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>empEdit</servlet-name>
		<servlet-class>com.bit.xml.EmpEdit</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>emplist</servlet-name>
		<url-pattern>/list.xml</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>empAdd</servlet-name>
		<url-pattern>/addXml.bit</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>empOne</servlet-name>
		<url-pattern>/emp.xml</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>empEdit</servlet-name>
		<url-pattern>/editXml.bit</url-pattern>
	</servlet-mapping>
</web-app>